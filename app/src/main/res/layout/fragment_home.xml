<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF3700B3">


    <TextView
        android:id="@+id/textViewDesc2"
        android:layout_width="371dp"
        android:layout_height="117dp"
        android:layout_marginHorizontal="40dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/roboto_black_italic"
        android:text="SkinSafe"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#9A7FFF"
        android:textSize="70dp" />

    <ImageView
        android:id="@+id/humanView"
        android:layout_width="374dp"
        android:layout_height="559dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="120dp" />

    <ImageView
        android:id="@+id/anterior_torso"
        android:layout_width="374dp"
        android:layout_height="559dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="120dp" />
    <ImageView
        android:id="@+id/head_neck"
        android:layout_width="374dp"
        android:layout_height="559dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="120dp" />
    <ImageView
        android:id="@+id/lateral_torso"
        android:layout_width="374dp"
        android:layout_height="559dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="100dp" />
    <ImageView
        android:id="@+id/lower_extremity"
        android:layout_width="374dp"
        android:layout_height="559dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="120dp" />
    <ImageView
        android:id="@+id/oral_genital"
        android:layout_width="374dp"
        android:layout_height="559dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="120dp" />
    <ImageView
        android:id="@+id/palms_soles"
        android:layout_width="374dp"
        android:layout_height="559dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="120dp" />

    <ImageView
        android:id="@+id/upper_extremity"
        android:layout_width="374dp"
        android:layout_height="559dp"
        android:layout_marginHorizontal="30dp"
        android:layout_marginTop="120dp" />

</RelativeLayout>